package com.voilaweb.tfd {	import mx.core.Container;		import twitter.api.data.TwitterStatus;		public class Storage {		private var _entries:Array;		private var _area:Container;				function Storage(area:Container) {			_area = area;			_entries = new Array();		}				public function get area():Container		{			return _area;		}				public function get entries():Array		{			return _entries;			}				public function push(entry:TwitterStatus):void		{			_entries.push(entry);		} 	}}