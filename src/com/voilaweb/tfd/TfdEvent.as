package com.voilaweb.tfd{	import flash.events.Event;		public class TfdEvent extends Event	{		private var _subType:String;		public function set subType(obj: String):void		{			_subType = obj;			}				public function get subType():String		{			return _subType;		}		private var _data:Array;		public function set data(obj: Array):void		{			_data = obj;			}				public function get data():Array		{			return _data;		}				public static const ON_ANYTHING    : String = "onAnything";		public static const ON_INIT_REPLY  : String = "onInitReply";		public static const ON_INIT_DIRECT : String = "onInitDirect";		public static const ON_FOLLOW_USER : String = "onFollowUser";		public static const ON_BLOCK_USER  : String = "onBlockUser";		public function TfdEvent (type : String, bubbles : Boolean = false, cancelable : Boolean = false)		{			super (type, bubbles, cancelable);		}			}}